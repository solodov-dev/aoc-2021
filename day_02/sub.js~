module.exports = {
    horizontal: 0,
    depth: 0,
    sail: (input) => {
	input.forEach(line => {
	    let [command, unit] = line.split(' ');
	    unit = Number(unit);
	    switch (command) {
	    case 'forward':
		this.horizontal + unit;
		break;
	    case 'down':
		this.depth + unit;
		break;
	    case 'up':
		this.depth - unit;
	    }
	})
    }
}
